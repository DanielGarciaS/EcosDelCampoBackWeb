{
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "https://ecosdelcampobackweb.onrender.com/api/:match*"
        },
        {
            "source": "/((?!.*\\.(?:html|css|js|png|jpg|jpeg|gif|svg|ico|json|woff|woff2|ttf|eot)).*)",
            "destination": "/index.html"
        }
    ]
}